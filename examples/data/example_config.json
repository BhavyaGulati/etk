{
  "conflict_handling": "replace",
  "exception_handling": "log",
  "extractions": [
    {
      "input": [
        "*.text.`parent`"
      ],
      "extractors": {
        "city": {
          "input_type": "tokens",
          "extractor": "extract_using_dictionary",
          "config": {
            "name": "cities"
          }
        },
        "hair_color": {
          "input_type": "tokens",
          "extractor": "extract_using_dictionary",
          "config": {
            "name": "ethnicities"
          }
        },
        "eye_color": {
          "input_type": "tokens",
          "extractor": "extract_using_dictionary",
          "config": {
            "name": "eyecolor"
          }
        },
        "names": {
          "input_type": "tokens",
          "extractor": "extract_using_dictionary",
          "config": {
            "name": "names"
          }
        }
      }
    },
    {
      "input": [
        "tables[*].rows[*].cells[*].text.`parent`"
      ],
      "extractors": {
        "city": {
          "input_type": "tokens",
          "extractor": "extract_using_dictionary",
          "config": {
            "name": "cities"
          }
        },
        "date_published": {
          "input_type": "text",
          "extractor": "extract_date"
        },
        "address": {
          "input_type": "text",
          "extractor": "extract_address"
        }
      }
    }
  ]
}