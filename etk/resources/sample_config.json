      "readability": [
        {
          "strict": "yes",
          "extraction_policy": "keep_existing|replace",
          "field_name": "content_strict"
        },
        {
          "strict": "no",
          "extraction_policy": "keep_existing|replace",
          "field_name": "content_relaxed"
        }
