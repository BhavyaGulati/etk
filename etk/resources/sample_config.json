  "data_extraction": [
    {
      "input_path": "*.text.`parent`",
      "name": {
        "extractors": {
          "extract_using_dict": {
            "config": {
              "dictionary": "_name_of_dictionary_in_resources_folder",
              "ngrams": 1,
              "joiner": " ",
              "pre_filter": [
                "x"
              ],
              "post_filter": [
                "isinstance(x, basestring)"
              ]
            },
            "extraction_policy": "keep_existing|replace"
          },
          "extract_using_regex": {
            "config": {
              "regex": "(?:my[\s]+name[\s]+is[\s]+([-a-z0-9@$!]+))",
              "regex_options": [
                "IGNORECASE",
                "S"
              ],
              "pre_filter": [
                "x.replace('\n', '')",
                "x.replace('\r', '')"
              ]
            },
            "extraction_policy": "keep_existing|replace"
          },
          "landmark": {
            "config": {
              "fields": [
                "inferlink_name",
                "inferlink_name-1"
              ],
              "post_filter": [
                "x.replace('\t', '')"
              ]
            },
            "extraction_policy": "keep_existing|replace"
          },
          "scrapy": {
            "config": {
              "entity_type": [
                "PER"
              ]
            },
            "extraction_policy": "keep_existing|replace"
          }
        }
      }
    },
    {
      "input_path": "content_relaxed.text.`parent`",
