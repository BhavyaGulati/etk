            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_height"
                ],
                "post_filter": [
                  "extract_height"
                ]
              }
            }
          }
        },
        "weight": {
          "extractors": {
            "extract_weight": {
              "config": {},
              "extraction_policy": "replace"
            },
            "extract_from_landmark": {
              "config": {
